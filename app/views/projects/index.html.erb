<h1>Main Dashboard</h1>
<hr>
<div>
  <h3>Projects</h3>
  <% @projects.each do |project| %>
  <p><b><%= link_to project.title, project_path(project) %></b></p>
  <p>Description: <%= project.description %></p>
  <p>End Date: <%= project.end_date %></p>
  <p>Client: <%= project.client %></p>
  <p>Revenue: <%= project.revenue %></p>
  <% end %>
</div>
<hr>
<div>
  <h3>Concepts</h3>
  <% @concepts.each do |concept| %>
  <p><b><%= concept.subject %></b></p>
  <p>Category: <%= concept.category %></p>
  <p>Subcategory: <%= concept.subcategory %></p>
  <p>Description: <%= concept.description %></p>
  <p>Amount: <%= concept.amount %></p>
  <% end %>
</div>